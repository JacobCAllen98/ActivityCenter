@model LoggedUser
<div class="col-sm-6">
    <div class="text-center">
        <h1>Login</h1>
    </div>
    <form action="check" method="post">
        <div class="form-group">
            <label>Email:</label>
            <span asp-validation-for="LogEmail" class="text-danger"></span>
            <input type="text" name="LogEmail" class="form-control">
        </div>
        <div class="form-group">
            <label>Password:</label>
            <span asp-validation-for="LogPassword" class="text-danger"></span>
            <input type="password" name="LogPassword" class="form-control">
        </div>
        <input type="submit" value="Login">
</div>
</form>
